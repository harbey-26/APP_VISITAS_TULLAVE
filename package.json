{
    "name": "app-visitas-tullave",
    "version": "1.0.0",
    "description": "Real Estate Visit Tracking Application",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "npx prisma db push --schema prisma/schema.pg.prisma && node prisma/seed.js && node server.js",
        "server": "node --watch server.js",
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint .",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "seed": "node prisma/seed.js",
        "postinstall": "prisma generate --schema prisma/schema.pg.prisma && npm run build",
        "db:deploy": "prisma migrate deploy --schema prisma/schema.pg.prisma"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.21.1",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "jsonwebtoken": "^9.0.2",
        "leaflet": "^1.9.4",
        "lucide-react": "^0.453.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-leaflet": "^4.2.1",
        "react-router-dom": "^6.26.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.47",
        "prisma": "^5.21.1",
        "tailwindcss": "^3.4.14",
        "vite": "^5.4.10"
    }
